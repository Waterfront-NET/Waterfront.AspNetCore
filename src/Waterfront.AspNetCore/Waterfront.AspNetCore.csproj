<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
        <DebugSymbols>true</DebugSymbols>
        <DebugType>portable</DebugType>

        <Version>$(SEMVER)</Version>
        <InformationalVersion>$(INFO_VER)</InformationalVersion>
    </PropertyGroup>

    <ItemGroup>
        <FrameworkReference Include="Microsoft.AspNetCore.App" Version="6.0.0"/>
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Waterfront.Common" Version="1.0.0-alpha.76"/>
        <PackageReference Include="Waterfront.Core" Version="1.0.0-alpha.76"/>
        <PackageReference Include="Waterfront.Extensions.DependencyInjection" Version="1.0.0-alpha.15"/>
    </ItemGroup>

</Project>
